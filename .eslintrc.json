{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "react-app"
    ],
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "rules": {
        // Ajoutez ici les règles spécifiques que vous souhaitez appliquer
    },
    "ignorePatterns": [
        "client/build/"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}